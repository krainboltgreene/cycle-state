{
  "name": "cycle-state",
  "version": "1.0.0",
  "description": "Create a stream of state for your view layer in cycle",
  "keywords": [
    "cycle",
    "state"
  ],
  "contributors": [
    {
      "name": "Kurtis Rainbolt-Greene",
      "email": "kurtis@rainbolt-greene.online",
      "url": "https://kurtis.rainbolt-greene.online"
    }
  ],
  "license": "ISC",
  "homepage": "https://github.com/krainboltgreene/cycle-state#readme",
  "repository": "github:krainboltgreene/cycle-state",
  "bugs": "https://github.com/krainboltgreene/cycle-state/issues",
  "main": "transpiled/index.js",
  "files": [
    "transpiled/"
  ],
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "**/test.js"
    ]
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "NODE_ENV=test tap --jobs-auto --100 './source/**/test.js'",
    "build": "babel './source/' -d './transpiled/'",
    "lint": "eslint './source/'",
    "check": "flow check",
    "ci": "npm run test && npm run lint && npm run check",
    "watch": "chokidar './source/' --command 'npm run ci --silent'"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "chokidar-cli": "1.2.0",
    "eslint": "4.10.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-promise": "3.6.0",
    "flow": "0.2.3",
    "flow-bin": "0.57.3",
    "flow-typed": "2.2.1",
    "nyc": "11.2.1",
    "tap": "10.7.2",
    "xstream": "11.0.0"
  },
  "dependencies": {
    "@unction/equals": "1.35.0",
    "@unction/istype": "2.7.0",
    "@unction/key": "4.6.0",
    "@unction/reducevalues": "2.33.0",
    "@unction/selectvalues": "1.27.0",
    "@unction/streamsatisfies": "3.31.0"
  }
}
